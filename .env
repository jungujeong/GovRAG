# LLM 모델 선택
#OLLAMA_MODEL=nous-hermes2:10.7b
#OLLAMA_MODEL=gemma3
OLLAMA_MODEL=qwen3:latest
#OLLAMA_MODEL=exaone3.5:7.8b
#OLLAMA_MODEL=solar:10.7b
#OLLAMA_MODEL=bnksys/yanolja-eeve-korean-instruct-10.8b:latest


# 임베딩 모델 선택
#EMBEDDING_MODELS=bona/bge-m3-korean:latest
EMBEDDING_MODELS=nomic-embed-text

OLLAMA_BASE_URL=http://localhost:11434
CHUNK_SIZE=1000
CHUNK_OVERLAP=200
TEMPERATURE=0.1
VECTOR_DB_PATH=./data/vector_db

# 자바 환경 설정 (Linux/Mac에서 hwplib 사용 시 필요)
# JAVA_HOME=/path/to/your/java
# 시스템에 설치된 JDK/JRE 경로를 지정하세요 (기본적으로 시스템 PATH에서 찾습니다)

# File size limits (in bytes)
MAX_FILE_SIZE=1048576  # 1MB (1024 * 1024 bytes)

